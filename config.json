{
    "description": "CAN driver for docker",
    "entrypoint": [
        "./netplugin"
    ],
    "network": {
        "type": "host"
    },
    "interface": {
        "types": [
            "docker.networkdriver/1.0"
        ],
        "socket": "dockercan.sock"
    },
    "linux": {
        "capabilities": [
            "CAP_NET_ADMIN"
        ]
    }
}